"""Lint our source files."""

import sys

import ssh_client
import libdot  # pylint: disable=wrong-import-order


def main(argv):
    """The main func!"""
    return libdot.lint.main(argv, basedir=ssh_client.DIR)


if __name__ == "__main__":
    sys.exit(main(sys.argv[1:]))
